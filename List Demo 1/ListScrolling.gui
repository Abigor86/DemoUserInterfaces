      <txt j="2" x="160" y="223" w="121" h="29" t="Medium_L" wrap="False" l="0">0</txt>
      <txt j="3" x="160" y="256" w="121" h="29" t="Medium_L" wrap="False" l="0">-</txt>
      <txt j="4" x="160" y="290" w="121" h="29" t="Medium_L" wrap="False" l="0">-</txt>
      <list j="10" x="860" y="165" w="164" h="603" t="list_000000" headerSub="" titleSub="" contentSub="ListItem_V" footerSub="" orientation="v" l="0" swipedelete="0" scrollCmd="VListDidScroll" scrollInterval="50" listEndCmd="VListDidScroll" listEndOffset="0" />
      <txt j="0" x="590" y="663" w="121" h="29" t="Medium_L" wrap="False" l="0"># items:</txt>
      <txt j="0" x="590" y="696" w="121" h="29" t="Medium_L" wrap="False" l="0">top row:</txt>
      <txt j="0" x="590" y="730" w="123" h="29" t="Medium_L" wrap="False" l="0">num visible:</txt>
      <txt j="12" x="722" y="663" w="121" h="29" t="Medium_L" wrap="False" l="0">0</txt>
      <txt j="13" x="722" y="696" w="121" h="29" t="Medium_L" wrap="False" l="0">-</txt>
      <txt j="14" x="722" y="730" w="121" h="29" t="Medium_L" wrap="False" l="0">-</txt>
      <slider j="5" d="0" x="346" y="260" w="300" h="13" min="0" max="100" decimals="0" pressCmd="DoScrollList" dragCmd="DoScrollList" unit="decimal" t="horizontal" sim="1" l="0" />
      <txt j="0" x="347" y="230" w="291" h="24" t="Small_C" wrap="False" l="0">Touch this slider to scroll the list above</txt>
      <txt j="0" x="25" y="352" w="613" h="236" t="Medium_L" wrap="False" l="0">This GUI demonstrates:
- Horizontal and vertical lists with external images
- Filling a list with Javascript
- Updating a slider from current list position using a command
- Interacting with the list from Javascript code
- Updating images using a refresh interval</txt>
    </landscape>
  </page>
  <subpage name="ListItem" w="128" h="130" t="Black_Background">
    <img x="4" y="5" w="120" h="120" j="1" clickthrough="1" refresh="20000" l="0" noCache="0" downloadIndicator="1" clearOnURLChange="0" />
