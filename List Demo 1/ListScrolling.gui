<gui>
  <properties>
    <project>ListTest</project>
    <designer>Florent Pillet / CommandFusion</designer>
    <size>
      <portrait width="768" height="1024" />
      <landscape width="1024" height="768" />
    </size>
    <imagefolder>
    </imagefolder>
    <debug loaderrors="0" connectionerrors="0" />
  </properties>
  <systems>
    <system name="Loopback" ip="localhost" port="0" protocol="tcp" alwayson="1" accept="0" ssl="0" maxconnections="0" heartbeatRx="" heartbeatTx="" heartbeatMode="0" eom="" connectionStatus="0" disconnectionStatus="0">
      <cmd name="HListDidScroll" js="onListDidScroll(JSON.parse(data));" jsSendsCommand="True" target="a5" value="{{min(65535, 65535*([top] / max(1, [count] - [visible])))}}">{"list":"[join]","top":[top],"visible":[visible],"count":[count]}</cmd>
      <cmd name="DoScrollList" js="onDoScrollList(&quot;l1&quot;, parseInt(data));" jsSendsCommand="True">[sliderval]</cmd>
      <cmd name="VListDidScroll" js="onListDidScroll(JSON.parse(data));" jsSendsCommand="True">{"list":"[join]","top":[top],"visible":[visible],"count":[count]}</cmd>
    </system>
  </systems>
  <themes>
    <theme type="background" name=".landscape"><![CDATA[ background-image: url(bg_landscape.png); background-repeat: no-repeat;]]></theme>
    <theme type="text" name=".Large_L"><![CDATA[ text-shadow: rgba(0,0,0,1) 1px -1px 1px; color: White; font-size: 34px; font-family: 'Arial'; text-align: left; vertical-align: middle; font-weight: bold; font-style: none; text-decoration: none; display: table-cell;]]></theme>
    <theme type="text" name=".Large_C"><![CDATA[ text-shadow: rgba(0,0,0,1) 1px -1px 1px; color: White; font-size: 34px; font-family: 'Arial'; text-align: center; vertical-align: middle; font-weight: bold; font-style: none; text-decoration: none; display: table-cell;]]></theme>
    <theme type="text" name=".Large_R"><![CDATA[ text-shadow: rgba(0,0,0,1) 1px -1px 1px; color: White; font-size: 34px; font-family: 'Arial'; text-align: right; vertical-align: middle; font-weight: bold; font-style: none; text-decoration: none; display: table-cell;]]></theme>
    <theme type="text" name=".Title_L"><![CDATA[ text-shadow: rgba(0,0,0,1) 1px -1px 1px; color: White; font-size: 24px; font-family: 'Arial'; text-align: left; vertical-align: middle; font-weight: bold; font-style: none; text-decoration: none; display: table-cell;]]></theme>
    <theme type="text" name=".Title_C"><![CDATA[ text-shadow: rgba(0,0,0,1) 1px -1px 1px; color: White; font-size: 24px; font-family: 'Arial'; text-align: center; vertical-align: middle; font-weight: bold; font-style: none; text-decoration: none; display: table-cell;]]></theme>
    <theme type="text" name=".Title_R"><![CDATA[ text-shadow: rgba(0,0,0,1) 1px -1px 1px; color: White; font-size: 24px; font-family: 'Arial'; text-align: right; vertical-align: middle; font-weight: bold; font-style: none; text-decoration: none; display: table-cell;]]></theme>
    <theme type="text" name=".Red_C"><![CDATA[ color: Red; font-size: 22px; font-family: 'Arial'; text-align: center; vertical-align: middle; font-weight: bold; font-style: none; text-decoration: none; display: table-cell;]]></theme>
    <theme type="gauge" name=".SliderTheme1[state='0']"><![CDATA[]]></theme>
    <theme type="gauge" name=".SliderTheme1[state='1']"><![CDATA[]]></theme>
    <theme type="gauge" name=".horizontal[state='0']"><![CDATA[ background-image: url(gauge_track_off.png);]]></theme>
    <theme type="gauge" name=".horizontal[state='1']"><![CDATA[ background-image: url(gauge_track_on.png);]]></theme>
    <theme type="text" name=".Medium_L"><![CDATA[ color: White; font-size: 22px; font-family: 'Arial'; text-align: left; vertical-align: top; font-weight: normal; font-style: none; text-decoration: none; display: table-cell;]]></theme>
    <theme type="text" name=".Large_C_copy"><![CDATA[ text-shadow: rgba(0,0,0,1) 1px -1px 1px; color: White; font-size: 34px; font-family: 'Arial'; text-align: center; vertical-align: middle; font-weight: bold; font-style: none; text-decoration: none; display: table-cell;]]></theme>
    <theme type="text" name=".Small_C"><![CDATA[ color: White; font-size: 12px; font-family: 'Arial'; text-align: center; vertical-align: middle; font-weight: normal; font-style: none; text-decoration: none; display: table-cell;]]></theme>
    <theme type="background" name=".Black_Background"><![CDATA[background-color: #000000;]]></theme>
    <theme type="list" name=".list_000000"><![CDATA[background-color: #000000;]]></theme>
    <theme type="list" name=".list_000000"><![CDATA[background-color: #000000;]]></theme>
  </themes>
  <scripts>
    <script name="listscrolling.js" />
  </scripts>
  <page name="Main" j="0" transition="None" subtype="None" time="0" ease="" start="1">
    <portrait />
    <landscape t="landscape">
      <list j="1" x="0" y="19" w="1024" h="130" t="list_000000" headerSub="" titleSub="" contentSub="ListItem" footerSub="" orientation="h" l="0" swipedelete="0" scrollCmd="HListDidScroll" scrollInterval="50" listEndCmd="HListDidScroll" listEndOffset="0" />
      <txt j="0" x="28" y="223" w="121" h="29" t="Medium_L" wrap="False" l="0"># items:</txt>
